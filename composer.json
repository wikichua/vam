{
    "name": "wikichua/vam",
    "description": "Vue Admin Manager + CRUD Generator for bootstrap Admin Panel Platform",
    "license": "MIT",
    "authors": [
        {
            "name": "Wiki Chua",
            "email": "wikichua@gmail.com",
            "homepage": "https://vam.latteichi.net"
        }
    ],
    "homepage": "https://github.com/wikichua/vam",
    "keywords": [
        "Laravel",
        "VAM",
        "Vue Admin Panel"
    ],
    "require": {
        "illuminate/support": "~5|~6|~7",
        "laravel/helpers": "^1.2",
        "tightenco/ziggy": "^0.9.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Wikichua\\VAM\\": "src/"
        },
        "files": [
            "src/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wikichua\\VAM\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wikichua\\VAM\\VAMServiceProvider"
            ],
            "aliases": {
                "VAM": "Wikichua\\VAM\\Facades\\VAM"
            }
        }
    }
}
